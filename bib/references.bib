@ARTICLE{Kroeker2014,
  title        = {{The role of temperature in determining species' vulnerability
                  to ocean acidification: a case study using Mytilus
                  galloprovincialis}},
  author       = {Kroeker, Kristy J and Gaylord, Brian and Hill, Tessa M and
                  Hosfelt, Jessica D and Miller, Seth H and Sanford, Eric},
