@ARTICLE{Angilletta2010,
  title        = {{Thermodynamic effects on organismal performance: is hotter
                  better?}},
  author       = {Angilletta, Jr, Michael J and Huey, Raymond B and Frazier,
                  Melanie R},
  journaltitle = {Physiological and biochemical zoology: PBZ},
