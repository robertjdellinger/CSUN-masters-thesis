}

@ARTICLE{Kordas2011,
  title        = {{Community ecology in a warming world: The influence of
                  temperature on interspecific interactions in marine systems}},
  author       = {Kordas, Rebecca L and Harley, Christopher D G and O'Connor,
                  Mary I},
  journaltitle = {Journal of experimental marine biology and ecology},
  publisher    = {Elsevier BV},
