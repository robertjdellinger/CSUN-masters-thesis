  author       = {Luan, Lu and Jiang, Yuji and Dini-Andreote, Francisco and
                  Crowther, Thomas W and Li, Pengfa and Bahram, Mohammad and
                  Zheng, Jie and Xu, Qinsong and Zhang, Xue-Xian and Sun, Bo},
  journaltitle = {Proceedings of the National Academy of Sciences of the United
