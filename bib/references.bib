@ARTICLE{Ries2009,
  title        = {{Marine calcifiers exhibit mixed responses to
                  {{{CO2-induced}}} ocean acidification}},
  author       = {Ries, Justin B and Cohen, Anne L and McCorkle, Daniel C},
  journaltitle = {Geology},
  publisher    = {GeoScienceWorld},
